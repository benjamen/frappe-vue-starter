{
  "doctype": "DocType",
  "name": "Property Estimate",
  "module": "Homemain",
  "custom": 0,
  "istable": 0,
  "allow_rename": 1,
  "track_changes": 1,
  "engine": "InnoDB",
  "autoname": "format:EST-{property}-{#####}",
  "title_field": "title",
  "search_fields": "property,source,estimate_date",
  "sort_field": "estimate_date",
  "sort_order": "DESC",
  "field_order": [
    "title",
    "property",
    "property_address",
    "estimate_date",
    "source",
    "section_break_1",
    "estimate_value",
    "estimate_value_display",
    "range_min",
    "range_max",
    "confidence_level",
    "section_break_2",
    "suburb_name",
    "suburb_average",
    "market_position",
    "section_break_3",
    "previous_estimate",
    "value_change",
    "change_percentage",
    "change_direction",
    "section_break_4",
    "raw_data",
    "notes"
  ],
  "fields": [
    {
      "label": "Title",
      "fieldname": "title",
      "fieldtype": "Data",
      "read_only": 1,
      "in_list_view": 1
    },
    {
      "label": "Property",
      "fieldname": "property",
      "fieldtype": "Link",
      "options": "Property",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "label": "Property Address",
      "fieldname": "property_address",
      "fieldtype": "Small Text",
      "fetch_from": "property.address",
      "read_only": 1
    },
    {
      "label": "Estimate Date",
      "fieldname": "estimate_date",
      "fieldtype": "Date",
      "reqd": 1,
      "in_list_view": 1,
      "default": "Today"
    },
    {
      "label": "Source",
      "fieldname": "source",
      "fieldtype": "Select",
      "options": "Homes.co.nz\nQV.co.nz\nTrademe Property\nCouncil Valuation\nOther",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldtype": "Section Break",
      "fieldname": "section_break_1",
      "label": "Estimate Details"
    },
    {
      "label": "Estimate Value",
      "fieldname": "estimate_value",
      "fieldtype": "Currency",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "label": "Estimate Display",
      "fieldname": "estimate_value_display",
      "fieldtype": "Data",
      "read_only": 1,
      "description": "Human-readable format like $840K"
    },
    {
      "label": "Range Minimum",
      "fieldname": "range_min",
      "fieldtype": "Currency"
    },
    {
      "label": "Range Maximum",
      "fieldname": "range_max",
      "fieldtype": "Currency"
    },
    {
      "label": "Confidence Level",
      "fieldname": "confidence_level",
      "fieldtype": "Select",
      "options": "High\nMedium\nLow",
      "default": "Medium"
    },
    {
      "fieldtype": "Section Break",
      "fieldname": "section_break_2",
      "label": "Market Context"
    },
    {
      "label": "Suburb",
      "fieldname": "suburb_name",
      "fieldtype": "Data"
    },
    {
      "label": "Suburb Average",
      "fieldname": "suburb_average",
      "fieldtype": "Currency"
    },
    {
      "label": "Market Position",
      "fieldname": "market_position",
      "fieldtype": "Select",
      "options": "Above Average\nAverage\nBelow Average",
      "description": "Position relative to suburb average"
    },
    {
      "fieldtype": "Section Break",
      "fieldname": "section_break_3",
      "label": "Change Analysis"
    },
    {
      "label": "Previous Estimate",
      "fieldname": "previous_estimate",
      "fieldtype": "Currency",
      "read_only": 1
    },
    {
      "label": "Value Change ($)",
      "fieldname": "value_change",
      "fieldtype": "Currency",
      "read_only": 1
    },
    {
      "label": "Change Percentage",
      "fieldname": "change_percentage",
      "fieldtype": "Float",
      "read_only": 1,
      "precision": "2"
    },
    {
      "label": "Direction",
      "fieldname": "change_direction",
      "fieldtype": "Select",
      "options": "Increase\nDecrease\nNo Change\nFirst Estimate",
      "read_only": 1
    },
    {
      "fieldtype": "Section Break",
      "fieldname": "section_break_4",
      "label": "Additional Information"
    },
    {
      "label": "Raw Data",
      "fieldname": "raw_data",
      "fieldtype": "JSON",
      "hidden": 1,
      "description": "Original scraped data for reference"
    },
    {
      "label": "Notes",
      "fieldname": "notes",
      "fieldtype": "Text"
    }
  ],
  "permissions": [
    {
      "role": "System Manager",
      "read": 1,
      "write": 1,
      "create": 1,
      "delete": 1,
      "email": 1,
      "export": 1,
      "print": 1,
      "report": 1,
      "share": 1
    }
  ],
  "actions": [],
  "links": [],
  "states": []
}
